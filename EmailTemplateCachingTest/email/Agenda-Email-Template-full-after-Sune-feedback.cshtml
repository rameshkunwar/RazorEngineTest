                                                                            <p class="h2" style="Margin:0;font-weight:400;padding-top:5px;font-size:14px; line-height:150%;Margin-bottom:10px;color:#33556f;">@Raw(item.PlanDescription)</p>
                                                                            @if (item.SubPlan != null)
                                                                             {
                                                                        <p style="Margin:0; Margin-bottom:10px;color:#33556f;">
                                                                            @*<span style="color:#002b4b;font-size:14px; font-weight:500;">Ritzau dækker</span> <br />*@
                                                                            <span style="color:#002b4b;font-size:14px; font-weight:500;">@item.RitzauOrFinansDaekker</span> <br />

                                                                            @foreach (var desc in item.SubPlan)
                                                                            {
                                                                                @*<span class="h2" style="font-size:14px; line-height:150%; font-weight:400;Margin-bottom:12px; padding-top:5px;color:#33556f;">@item.SubPlan.Description </span>*@
