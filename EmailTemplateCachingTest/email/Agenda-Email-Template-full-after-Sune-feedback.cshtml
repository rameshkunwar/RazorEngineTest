            }

            .h3 {
                font-size: 16px;
                font-weight: 600;
                Margin-bottom: 12px;
            }

            .one-column .contents {
                text-align: left;
            }

            .one-column p {
                font-size: 16px;
                Margin-bottom: 10px;
                color: #33556f;
            }

            .two-column {
                font-size: 0;
            }

                .two-column .column {
                    width: 100%;
                    max-width: 300px;
                    display: inline-block;
                    vertical-align: top;
                }

            .header-text {
                padding-top: 10px;
            }

            .col-pipe {
                width: 15px;
                max-width: 100%;
                display: inline-block;
                vertical-align: top;
            }

            .c-d-c-h {
                width: 96%;
                max-width: 100%;
                display: inline-block;
                vertical-align: top;
