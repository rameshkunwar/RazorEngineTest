                                                                        <tr>
                                                                            <td valign="top" class="inner-tc-head" style="padding:0 !important; Margin:0 !important;">
                                                                                <table  class="contents" style="border-spacing:0;font-family:'Roboto','Verdana',Helvetica,Arial,sans-serif;word-break:break-word;line-height:150%;width:100%; text-align:left;">
                                                                                    <tr>
                                                                                        
                                                                                        <td class="text" style="padding:5px 0 0 2px !important;">
                                                                                            <span style="color:#657f92;font-weight:400;font-size:16px;">@item.PlanTime @item.City</span>
                                                                                            @if (@item.PlanTime != "" || @item.City != "")
                                                                                            {
                                                                                                <span style="padding-left:2px;padding-right:2px;">&nbsp;</span>
                                                                                            }
                                                                                            <span style="color:#002b4b;font-size:16px;">@item.PlanHeader</span>
                                                                                        </td>

                                                                                    </tr>
                                                                                </table>
                                                                            </td>
                                                                        </tr>
                                                                    </table>
                                                                </div>


                                                                <!--[if (gte mso 9)|(IE)]>
                                                                </td>
                                                                </tr>
                                                                </table>
                                                                <![endif]-->

                                                            </td>
                                                        </tr>
                                                        
                                                        <!--INDIVIDUAL PLAN DESCRIPTION ALONG WITH DATE AND CITY-->
