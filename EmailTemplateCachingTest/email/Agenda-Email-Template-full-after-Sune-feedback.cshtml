                                                                        <tr>
                                                                            <td class="i-stof" style="padding-bottom:0;padding-left:0;padding-right:10px;padding-top:5px;">
                                                                                <table class="xxcontentsxx" style="border-spacing:0;text-align:left;  width:100%;">
                                                                                    <tr>
                                                                                        <td class="text stof" style="text-align:right; padding:0;color:#657f92;font-weight:400;padding-top:5px;">
                                                                                        <span style="font-size:14px;color:#33556F;">@item.PlanStoftype</span></td>
                                                                                    </tr>
                                                                                </table>
                                                                            </td>
                                                                        </tr>
                                                                    </table>
                                                                </div>


                                                                <!--[if (gte mso 9)|(IE)]>
                                                                </td>
                                                                </tr>
                                                                </table>
                                                                <![endif]-->

                                                            </td>

                                                        </tr>
                                                        <!--/TJENESTERTYPER AND STOFTYPE-->
                                                        <!--GRAY BORDER LINE AFTER EVERY PLAN-->
                                                        <tr>
                                                            <td class="one-column" style="padding:0;">
                                                                <table width="100%" style="border-spacing:0;background-color:#ebeff2;padding:5px;">                                                                    

                                                                </table>
                                                            </td>
                                                        </tr>

                                                    </table>

                                                </td>
                                            </tr>



                                        }
                                    }

                                </table>
                            </td>
                        </tr>
                    }
