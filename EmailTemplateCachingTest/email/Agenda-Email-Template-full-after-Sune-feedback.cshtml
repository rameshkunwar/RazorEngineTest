            }

            .col-tjen {
                width: 65%;
                max-width: 100%;
                display: inline-block;
                vertical-align: top;
            }

            .tjen {
                padding-left: 11px;
                padding-top: 5px;
            }

            .col-stof {
                width: 35%;
                max-width: 100%;
                display: inline-block;
                vertical-align: top;
            }

            .two-column .contents {
                font-size: 14px;
                text-align: left;
            }

            .header, .date {
                background-color: #ebeff2;
            }

            .t-c, .stof {
                color: #657f92;
                font-weight: 600;
            }

            .inner-tc-head {
                padding-top: 2px;
            }

            .two-column .text {
                padding-top: 5px;
            }

            .hr-line {
                min-width: 100%;
